
    <h1> <%= username %>'s WishList</h1>
    
    <div class="d-flex flex-wrap justify-content-left">
    <% for (item of items){%>
        
        <div class="p-3 m-3 border border-primary bg-light d-flex flex-wrap justify-content-left" style="width:175px;position:relative;">
            <a class="ml-5 btn btn-warning" href="/wishlist/delete/<%= item._id%>" style="position:absolute;bottom:2px;right:2px;font-size:10px"> - </a>
            <strong class="pr-5"><%= item.title %></strong><br>
            <p class="pt-1"><%= item.author %></p> <br>
            <a class="btn" href="/books/show/<%= item.isbn %>"><img class="pl-5" src="<%= item.image %>" style="width:100px"></a>
            
            
        </div>
        </a>
    <% } %>
</div>