<h1>Search Result</h1>
<hr>
<div class="d-flex flex-wrap justify-content-left">
<% for (var i=0; i<titleRes.length; i++) { %>
    <div class="m-3 card" style="width: 20rem;">
        <img src="<%= imageRes[i] %>" style="padding:10px" class="card-img-top" alt="<%= titleRes[i] %>">
        <div class="card-body">
          <h2 class="card-title"><%= titleRes[i] %></h2>
          <% for (var j=0; j<authorRes[i].length; j++) { %>
            <h5 class="card-title"><%= authorRes[i][j] %></h5>
          <% } %>
          <p class="card-text" style="font-size:15px"><%= descriptionRes[i].substring(0, 150) %>...</p>
          <a href="/books/show/<%= titleRes[i] %>/<%= authorRes[i] %>">Read More>></a>
          <a href="#" class="mt-3 btn btn-danger" style="position:absolute;top:2px;right:15px;font-size:20px">+</a>
        </div>
      </div>
<% } %>
</div>
<pre>
    
</pre>